#!/bin/bash
export smtpemailfrom='FLISOL2014<rauhmaru@opensuse.org>'
export zabbixemailto=$1
export zabbixsubject=$2
export zabbixbody=$3
export smtpserver=smtp.gmail.com
export smtplogin=rauhmaru@gmail.com
export smtppass=l3G3nd4ry

/usr/bin/sendEmail -f $smtpemailfrom -t $zabbixemailto -u $zabbixsubject -m $zabbixbody -s $smtpserver:25 -xu $smtplogin -xp $smtppass

